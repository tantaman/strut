{
  "compilerOptions": {
    "sourceMap": true,
    "strictNullChecks": true,
    "module": "esnext",
    "target": "esnext",
    "skipLibCheck": true,
    "moduleResolution": "Node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "baseUrl": "./src/",
    "declaration": true,
    "allowJs": true,
    "composite": true,
    "declarationMap": true,
    "incremental": true,
    "outDir": "./lib/",
    "rootDir": "./src"
  },
  "include": ["./src/"],
  "references": []
}
